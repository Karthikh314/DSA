class Solution:
    def fib(self, n):
        if n == 0:
            return 0
        elif n == 1:
            return 1
        return self.fib(n - 1) + self.fib(n - 2)

if __name__ == "__main__":
    solution = Solution()
    n = 5  # Example input
    print(f"Fibonacci number at position {n} is {solution.fibonacci(n)}")
