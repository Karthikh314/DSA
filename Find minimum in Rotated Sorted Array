class Solution:
    def findMin(self, arr):
        mini = float('inf') 
        for num in arr:
            
            mini = min(mini, num)
        return mini 

if __name__ == "__main__":
    arr = [4, 5, 6, 7, 0, 1, 2, 3]
    
     # Create an object of the Solution class
    sol = Solution()
    
    ans = sol.findMin(arr) 
    
    # Print the result
    print("The minimum element is:", ans) 
