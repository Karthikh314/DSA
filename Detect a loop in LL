
 class ListNode:
     def __init__(self, val=0, next=None):
         self.val = val
         self.next = next

class Solution:
    def hasCycle(self, head):
        temp = head  
        nodeSet = set()  
        while temp is not None:
            if temp in nodeSet:
                return True
            nodeSet.add(temp)
            temp = temp.next  
        return False

def printLinkedList(head):
    temp = head
    while temp is not None:
        print(temp.val, end=" ")
        temp = temp.next
    print()

def main():
    head = ListNode(1)
    second = ListNode(2)
    third = ListNode(3)
    fourth = ListNode(4)
    fifth = ListNode(5)

    head.next = second
    second.next = third
    third.next = fourth
    fourth.next = fifth
    fifth.next = third 

    sol = Solution()
    if sol.hasCycle(head):
        print("Loop detected in the linked list.")
    else:
        print("No loop detected in the linked list.")

if __name__ == "__main__":
    main()
