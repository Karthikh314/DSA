class Solution:
    def searchInsert(self, nums, target):
        for i in range(len(nums)):
            if nums[i] >= target:
                # Return the current index
                return i
        return len(nums)

nums = [1, 3, 5, 6]
target = 5

sol = Solution()
index = sol.searchInsert(nums, target)
print(f"The index is: {index}")
