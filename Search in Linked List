class Node:
    def __init__(self, data):
        self.data = data      
        self.next = None   
class Solution:
    def searchKey(self, head, key):
        current = head   # Start from head node
        
        while current is not None:
            if current.val == key:   # If key is found
                return True
            current = current.next    # Move to next node
        
        return False


    
