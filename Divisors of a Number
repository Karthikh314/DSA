class Solution:
    def divisors(self, n):
        ans = []
        for i in range(1, n + 1):
            if n % i == 0:
                ans.append(i)
        return 
n = 6
sol = Solution()
ans = sol.divisors(n)

print(f"The divisors of {n} are: ", end="")
for i in range(len(ans)):
    print(ans[i], end=" ")
