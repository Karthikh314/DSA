from typing import List
class Solution:
    def sortZeroOneTwo(self, nums):
        nums.sort()

# Main function
if __name__ == "__main__":
    nums = [2, 0, 1, 1, 0, 2]
    
    # Create an instance of Solution class
    sol = Solution()
    
    sol.sortZeroOneTwo(nums)
    
    # Print the array elements
    print(" ".join(map(str, nums)))
