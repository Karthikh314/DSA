class Solution:
    def printX(self, X, N)
        if N < 0:
            print("Invalid number of times")
            return
        for i in range(N):
            print(X, end='')
            
        
            if i < N - 1:
                print(" ", end='')
      
        print()

sol = Solution()
X = 7
N = 5

sol.printX(X, N)
