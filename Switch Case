class Solution:
    def whichWeekDay(self, day):
        if day < 1 or day > 7:
            print("Invalid")
            return
        match day:
            case 1:
                print("Monday")
            case 2:
                print("Tuesday")
            case 3:
                print("Wednesday")
            case 4:
                print("Thursday")
            case 5:
                print("Friday")
            case 6:
                print("Saturday")
            case 7:
                print("Sunday")

sol = Solution()

day = int(input("Enter the day number: "))

sol.whichWeekDay(day)
