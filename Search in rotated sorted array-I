class Solution:
    def search(self, nums, target):
        # Get the size of the array
        n = len(nums)

        for i in range(n):
            if nums[i] == target:
                return i
        return -1

if __name__ == "__main__":
    nums = [4, 5, 6, 7, 0, 1, 2]
    target = 0
    sol = Solution()
    result = sol.search(nums, target)

    if result == -1:
        print("Target is not present.")
    else:
        print("The index is:", result)
